{
  "manifest_version": 2,
  "name": "Auto Tab Switcher",
  "version": "1.0",
  "description": "定时切换标签页的插件",
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  }
}